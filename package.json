{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "pre-commit": "next lint --fix && git add -A .",
    "tsc": "tsc --watch --noEmit",
    "test": "yarn test:dom && yarn test:node",
    "test:cov": "yarn test:dom --coverage && yarn test:node --coverage",
    "test:dom": "jest --config ./jest.config.dom.js",
    "test:dom:watch": "yarn test:dom --watch",
    "test:node": "jest --config ./jest.config.node.js",
    "test:node:watch": "yarn test:node --watch"
  },
  "dependencies": {
    "@prisma/client": "^2.28.0",
    "@reach/auto-id": "^0.16.0",
    "msw": "^0.34.0",
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "^3.19.2",
    "zod": "^3.7.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/faker": "^5.5.7",
    "@types/node": "^16.4.13",
    "@types/react": "17.0.16",
    "babel-jest": "^27.0.6",
    "eslint": "7.32.0",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-testing-library": "^4.10.1",
    "faker": "^5.5.3",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.2",
    "prisma": "^2.28.0",
    "type-fest": "^2.0.0",
    "typescript": "4.3.5",
    "whatwg-fetch": "^3.6.2"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
